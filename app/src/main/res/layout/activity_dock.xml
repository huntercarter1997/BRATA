<?xml version="1.0" encoding="utf-8"?>
<!--suppress AndroidLintHardcodedText -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
    <TextView
        android:id="@+id/textViewDockTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:textAppearance="@android:style/TextAppearance.Large"
        android:layout_margin="8dip"
        android:text="Dock"
        android:textStyle="bold" />

    <Button
        android:id="@+id/buttonDockStartChallenge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewDockTitle"
        android:layout_toLeftOf="@+id/textViewDockTitle"
        android:text="Start Challenge" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:layout_marginTop="43dp"
        android:layout_below="@+id/buttonDockStartChallenge"
        android:layout_alignLeft="@+id/buttonDockStartChallenge"
        android:id="@+id/textViewDockDecodedMessage"
        android:editable="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewDockFuelInitial"
        android:layout_below="@+id/textViewDockFuelConsumption"
        android:layout_alignLeft="@+id/textViewDockFuelConsumption"
        android:layout_marginTop="31dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewDockFuelConsumption"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/textViewDockDecelerationForce" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewDockAccelerationForce"
        android:layout_marginTop="30dp"
        android:layout_below="@+id/textViewDockDecodedMessage"
        android:layout_alignLeft="@+id/textViewDockDecodedMessage" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewDockDecelerationForce"
        android:layout_marginTop="31dp"
        android:layout_below="@+id/textViewDockAccelerationForce"
        android:layout_alignLeft="@+id/textViewDockAccelerationForce" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/buttonDockLaunchStadimeter"
        android:layout_alignBottom="@+id/buttonDockStartChallenge"
        android:layout_toRightOf="@+id/textViewDockTitle" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:layout_below="@+id/textViewDockFuelInitial"
        android:layout_alignLeft="@+id/textViewDockFuelInitial"
        android:layout_marginTop="35dp"
        android:id="@+id/textViewDockDistance" />

</RelativeLayout>
